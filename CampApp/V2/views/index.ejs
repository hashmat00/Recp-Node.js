<% include partials/header %>

<div class="container">
           <header class="jumbotron">
               <div class="container">
                <h1>Welcome to Camping</h1>
                <p>View our camping lsiting ...</p>
        
                <p>
                 <a class="btn btn-primary btn-large" href="/camp/new">Add new Camp</a>
                </p>
            </div>
      </header>
        
        
      <!--<div class="row" style="display:flex; flex-wrap; wrap;">-->
             <div class="row">

      
      
        <% camp.forEach(function(camp) { %>
            <div class="col-md-3 col-sm-6">
                <div class="thumbnail">
                   <img src="<%= camp.image %>">
                       <div class="caption">
                         <h4><%= camp.name %></h4>
                         <p>
                             <a href="/camp/<%= camp._id %>" class="btn btn-primary">View Camp</a>
                         </p>
                       </div>
                </div>
                 
              </div>
        <% }); %>
  
  
    </div>
      
     
      
      
      
</div>










<% include partials/footer %>